{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
		"paths": {
			"$lib": ["./src/lib"],
            "$lib/*": ["./src/lib/*"],
			"$cmp": ["./src/components"],
            "$cmp/*": ["./src/components/*"],
		}
	}
}
