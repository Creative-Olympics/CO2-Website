{
	"name": "co-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:only": "vite dev",
		"build:only": "vite build",
		"preview": "vite preview",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/app.css -o src/app-output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/app.css -o src/app-output.css",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"devhost": "concurrently \"npm run dev:only -- --host\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@mdi/js": "^7.2.96",
		"@playwright/test": "^1.35.0",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "^1.20.2",
		"@tailwindcss/typography": "^0.5.9",
		"@types/cookie": "^0.5.1",
		"@zerodevx/svelte-img": "^1.2.10",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.1",
		"svelthree": "^0.119.0-5",
		"tailwindcss": "^3.3.2",
		"vite": "^4.3.9",
		"vite-plugin-minify": "^1.5.2",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"current-year": "^1.1.12",
		"daisyui": "^2.52.0",
		"firebase": "^9.22.2",
		"is-ios": "^2.1.0",
		"locomotive-scroll": "^4.1.4",
		"rxfire": "^6.0.3",
		"rxjs": "^7.8.1",
		"skinview3d": "^3.0.0-alpha.1",
		"svelte-youtube": "^0.0.2",
		"three": "^0.148.0"
	}
}
